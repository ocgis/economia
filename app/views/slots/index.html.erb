<p id="notice"><%= notice %></p>

<h1>Slots</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Value</th>
      <th>Value gdate</th>
      <th>Value slot key</th>
      <th>Value slot value</th>
      <th>Value slot value slot key</th>
      <th>Value slot value slot value</th>
      <th>Etransaction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slots.each do |slot| %>
      <tr>
        <td><%= slot.key %></td>
        <td><%= slot.value %></td>
        <td><%= slot.value_gdate %></td>
        <td><%= slot.value_slot_key %></td>
        <td><%= slot.value_slot_value %></td>
        <td><%= slot.value_slot_value_slot_key %></td>
        <td><%= slot.value_slot_value_slot_value %></td>
        <td><%= slot.etransaction %></td>
        <td><%= link_to 'Show', slot %></td>
        <td><%= link_to 'Edit', edit_slot_path(slot) %></td>
        <td><%= link_to 'Destroy', slot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slot', new_slot_path %>
