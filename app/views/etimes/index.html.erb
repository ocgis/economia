<p id="notice"><%= notice %></p>

<h1>Etimes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @etimes.each do |etime| %>
      <tr>
        <td><%= etime.date %></td>
        <td><%= link_to 'Show', etime %></td>
        <td><%= link_to 'Edit', edit_etime_path(etime) %></td>
        <td><%= link_to 'Destroy', etime, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Etime', new_etime_path %>
