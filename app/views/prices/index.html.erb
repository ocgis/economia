<p id="notice"><%= notice %></p>

<h1>Prices</h1>

<table>
  <thead>
    <tr>
      <th>Id </th>
      <th>Source</th>
      <th>Value</th>
      <th>Commodity id </th>
      <th>Commodity space</th>
      <th>Currency id </th>
      <th>Currency space</th>
      <th>Time date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.id_ %></td>
        <td><%= price.source %></td>
        <td><%= price.value %></td>
        <td><%= price.commodity_id_ %></td>
        <td><%= price.commodity_space %></td>
        <td><%= price.currency_id_ %></td>
        <td><%= price.currency_space %></td>
        <td><%= price.time_date %></td>
        <td><%= link_to 'Show', price %></td>
        <td><%= link_to 'Edit', edit_price_path(price) %></td>
        <td><%= link_to 'Destroy', price, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Price', new_price_path %>
