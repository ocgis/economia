<p id="notice"><%= notice %></p>

<h1>Reconcile Dates</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reconcile_dates.each do |reconcile_date| %>
      <tr>
        <td><%= reconcile_date.date %></td>
        <td><%= link_to 'Show', reconcile_date %></td>
        <td><%= link_to 'Edit', edit_reconcile_date_path(reconcile_date) %></td>
        <td><%= link_to 'Destroy', reconcile_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reconcile Date', new_reconcile_date_path %>
