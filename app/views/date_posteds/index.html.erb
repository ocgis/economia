<p id="notice"><%= notice %></p>

<h1>Date Posteds</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_posteds.each do |date_posted| %>
      <tr>
        <td><%= date_posted.date %></td>
        <td><%= link_to 'Show', date_posted %></td>
        <td><%= link_to 'Edit', edit_date_posted_path(date_posted) %></td>
        <td><%= link_to 'Destroy', date_posted, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date Posted', new_date_posted_path %>
